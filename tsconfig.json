{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/__tests__/ThemedText-test.jsx",
    "app/+not-found.jsx",
    "app/+html.jsx",
    "app/_layout.jsx",
    "app/(tabs)/index.jsx",
    "app/(tabs)/explore.jsx",
    "app/(tabs)/_layout.jsx",
    "components/ParallaxScrollView.jsx",
    "components/HelloWave.jsx",
    "components/ExternalLink.jsx",
    "components/Collapsible.jsx",
    "components/navigation/TabBarIcon.jsx",
    "components/ThemedText.jsx",
    "components/ThemedView.jsx",
    "hooks/useThemeColor.js",
    "hooks/useColorScheme.web.js",
    "hooks/useColorScheme.js",
    "constants/Colors.js"
  ]
}
